efault: ''\n    },\n    futureDayHide: {\n      type: String,\n      default: '2554387200'\n    },\n    defaultShowDate: {\n      type: Number\n    }\n  },\n  data: function data() {\n    return {\n      // 存储选中的值 和 默认选中的值\n      values: '',\n      oldvalues: '',\n      // 控制下拉框是都可见\n      visible: false,\n      // 选项列表是否有数据\n      isNotOption: false,\n      // isFocused\n      isFocused: false,\n      // 是否设置成错误状态\n      isError: false,\n      nowV: ''\n    };\n  },\n  mounted: function mounted() {\n    if (this.value) {\n      this.values = this.value.replace(/\\//g, '-');\n      this.nowV = this.values;\n    }\n  },\n  computed: {\n    currentLan: function currentLan() {\n      if (this.$store.state.baseData) {\n        return this.$store.state.baseData.lan;\n      }\n\n      return null;\n    },\n    currentDate: function currentDate() {\n      var date = new Date(this.value);\n      return {\n        year: date.getFullYear(),\n        week: date.getDay(),\n        month: date.getMonth(),\n        day: date.getDate()\n      };\n    },\n    previewDateText: function previewDateText() {\n      if (!this.value) return '';\n      var isChinese = this.currentLan === 'zh_CN';\n      var weekText = this.$t(\"weeks.\".concat(this.currentDate.week));\n      var month = this.$t(\"months.\".concat(this.currentDate.month));\n      var day = \"\".concat(this.currentDate.day).concat(isChinese ? '日' : ''); // 2019-02-01\n\n      var en = \"\".concat(weekText, \",\").concat(day, \" \").concat(month);\n      var zh = \"\".concat(month).concat(day, \"\\uFF0C\").concat(weekText);\n      return isChinese ? zh : en;\n    },\n    classes: function classes() {\n      return ['common-select', {\n        'select-visible': this.visible,\n        'select-disabled': this.disabled,\n        'select-value': this.values,\n        'select-error': this.isError\n      }];\n    },\n    stylees: function stylees() {\n      var styles = this.styles || {};\n      if (this.width) styles.width = this.width;\n      if (this.height) styles.width = this.height;\n      return styles;\n    }\n  },\n  watch: {\n    value: function value(data) {\n      if (!data) this.values = '';else if (this.value) {\n        this.values = data.replace(/\\//g, '-');\n      }\n    },\n    visible: function visible(value) {\n      this.$emit('opent-chang', value, this.name);\n    },\n    errorFlag: function errorFlag(value) {\n      this.isError = value;\n    }\n  },\n  methods: {\n    clickDay: function clickDay(data) {\n      this.nowV = data.replace(/\\//g, '-');\n      this.$emit('onChanges', data.replace(/\\//g, '-'), this.name);\n    },\n\n    /* changeDate(data) {\n      console.log(data); // 左右点击切换月份\n    }, */\n    // 下拉框 显示&隐藏\n    toggleMenu: function toggleMenu() {\n      this.visible = !this.visible;\n    },\n    // 点击 select 以外 隐藏下拉框\n    onClickOutside: function onClickOutside() {\n      this.oldvalues = this.values;\n      this.visible = false;\n    },\n    // 点击 选项\n    onOptionClick: function onOptionClick(item) {\n      this.visible = false;\n      this.isError = false;\n      this.$emit('onChanges', item, this.name);\n    },\n    // 清除选项\n    resetOptionData: function resetOptionData() {\n      this.values = '';\n      this.$emit('onClear', true, this.name);\n    },\n    // 取消选择日期\n    cancelCalendar: function cancelCalendar() {\n      this.$emit('onChanges', this.oldvalues, this.name);\n      this.visible = false;\n      this.isError = false;\n    },\n    confirmCalendar: function confirmCalendar() {\n      this.oldvalues = this.values;\n      this.visible = false;\n      this.$emit('onSelect', this.nowV, this.name);\n    }\n  }\n};",{"version":3,"sources":["index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4EA,SAAA,SAAA,IAAA,YAAA,QAAA,mBAAA;AACA,OAAA,OAAA;AACA,OAAA,QAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,YADA;AAEA,EAAA,UAAA,EAAA;AACA,IAAA,QAAA,EAAA,QADA;AAEA,IAAA,OAAA,EAAA;AAFA,GAFA;AAMA,EAAA,UAAA,EAAA;A