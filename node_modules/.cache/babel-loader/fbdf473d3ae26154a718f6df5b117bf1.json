","sourcesContent":["<template>\n  <section class=\"bitsg-swiper-base\" style=\"background-color:#0e1a2e;\">\n    <!-- 解释文案 -->\n    <div class=\"text\" v-html=\"slideListTitle.title\"></div>\n    <!-- title -->\n    <!-- <div class=\"title b-8-cl\">{{slideListTitle.title}}</div> -->\n    <!-- sub -->\n    <div class=\"subtitle\">{{slideListTitle.subTitle}}</div>\n    <!-- button -->\n    <div class= 'goSignBut' >\n      <button v-if=\"!isLogin && userInfoIsReady\"\n      class=\"common-button common-button-solid u-8-bg u-16-cl \"\n      @click=\"goSign\">{{ $t(\"login.goRegister\") }}</button>\n    </div>\n    <!-- 图片区域 -->\n    <div class=\"imgBox\" v-if=\"imgs.length\">\n      <!-- 图片 -->\n      <ul class=\"img-ul clearfix\">\n        <transition-group name=\"aaa\">\n          <li v-for=\"(item, index) in imgList\" :key=\"`${page}${index}`\"\n              class=\"u-8-bd\"\n              @click=\"goTo(item.httpUrl)\"\n              @mouseenter=\"enter\"\n              @mouseleave=\"leave\">\n            <img :src=\"item.imageUrl\" alt=\"\">\n          </li>\n        </transition-group>\n      </ul>\n      <!-- 锚点 -->\n      <div class=\"sNav\">\n        <ul class=\"img-pages clearfix\" v-if=\"imgs.length > 4\">\n          <li v-for=\"(item, index) in pageLength\" :key=\"index\"\n              :class=\"(index + 1) === page ? 'active' : ''\"\n              @click=\"pageClick(index + 1)\">\n            <span class=\"pageBg\"></span>\n            <span v-if=\"(index + 1) === page\" class=\"pageBar\" :style=\"{width: pageWidth + 'px'}\">\n            </span>\n          </li>\n        </ul>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\nimport commonMixin from 'BlockChain-ui-privatization/PC/common-mixin/home/swiper/bitsg/bitsg';\nimport 'BlockChain-ui-privatization/PC/common-mixin/home/swiper/bitsg/bitsg.styl';\n\nexport default {\n  mixins: [commonMixin],\n  created() {\n    this.init();\n  },\n\n};\n</script>\n"],"sourceRoot":"app/view/src/views/home/swiper"}]}