vnode.context.focus();\n            }\n          } else if (vnode.context.isVisable) {\n            vnode.context.blur();\n          }\n        };\n        // eslint-disable-next-line no-param-reassign\n        el.handler = handler;\n        document.addEventListener('click', handler);\n      },\n      unbind(el) {\n        document.removeEventListener('click', el.handler);\n      },\n    },\n  },\n  methods: {\n    focus() {\n      this.isVisable = true;\n    },\n    blur() {\n      this.isVisable = false;\n    },\n    handleChoose(val) {\n      this.$emit('onSelectChange', val);\n      this.blur(); // 关闭面板\n    },\n  },\n};\n</script>\n<style>\n.simple__select {\n  width: fit-content;\n  position: relative;\n}\n.select__container {\n  height: 28px;\n  /* padding: 10px; */\n  border: 1px solid rgba(255, 255, 255, .3);\n  display: flex;\n  position: relative;\n  width: fit-content;\n  cursor: pointer;\n}\n.select__container input {\n  display: block;\n  height: 28px;\n  width: 100%;\n  font-family: PingFangSC-Regular;\n  font-size: 12px;\n  color: #FFFFFF;\n  letter-spacing: 0.12px;\n  line-height: 14px;\n  font-weight: 400;\n  text-align: center;\n  padding: 0 20px 0 10px;\n  cursor: pointer;\n}\n.select__container img {\n  position: absolute;\n  right: 8px;\n  top: 12px;\n  width: 14px;\n  height: fit-content;\n}\n.select__options {\n  width: 100%;\n  position: absolute;\n  top: 28px;\n  left: 0;\n  border: 1px solid rgba(255, 255, 255, .3);\n  box-sizing: border-box;\n  z-index: 100;\n}\n.select__options li {\n  font-family: PingFangSC-Regular;\n  font-size: 12px;\n  color: #FFFFFF;\n  letter-spacing: 0.12px;\n  line-height: 14px;\n  font-weight: 400;\n  /* margin-top: 6px; */\n  text-align: center;\n  padding: 5px 0;\n  z-index: 100;\n  cursor: pointer;\n}\n.select__options li.select, .select__options li:hover {\n  background: rgba(255,255,255,0.23);\n}\n\n</style>\n"],"sourceRoot":"../BlockChain-ui/PC/components/simpleSelect"}]}