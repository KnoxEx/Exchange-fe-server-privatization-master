.currentSymbol {
    height: 69px;
    border-bottom-width: 1px;
    border-bottom-style: solid;
    .symbol-total {
      line-height: 69px;
      float: left;
      padding-left: 16px;
      span {
        vertical-align: middle;
      }
      .symbol-name {
        font-size: 18px;
      }
      .symbol-number {
        font-size: 16px;
        margin-left: 5px;
      }
      .symbol-property {
        font-size: 12px;
      }
    }
    .symbol-info {
      float: right;
      padding: 18px 16px 0 0;
      height: 34px;
      .info-item {
        float: left;
        margin-left: 20px;
        font-size: 12px;
        text-align: right;
        span  {
          display: block;

        }
        .vla {
          margin-top: 10px;
        }
      }
    }
  }