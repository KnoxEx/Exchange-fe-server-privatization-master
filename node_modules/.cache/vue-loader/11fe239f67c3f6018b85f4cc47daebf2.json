e>\n  <div\n    v-if=\"viewShow\"\n    class=\"company-application-page company-application-container\"\n  >\n    <div class=\"company-application a-5-bg\">\n      <div class=\"application-header\">\n        <h1 class=\"b-1-cl title\">{{ $t(\"application.applyCompany\") }}</h1>\n        <p class=\"b-2-cl desc\">{{ $t(\"application.applyCompanyDesc\") }}</p>\n      </div>\n      <ul class=\"service-list\">\n        <li\n          class=\"li a-4-bg\"\n          v-for=\"(item, inx) in serviceList\"\n          :key=\"`serviceList${inx}`\"\n        >\n          <img :src=\"imgList[inx]\" class=\"icon\" />\n          <h3 class=\"b-1-cl li-title\">{{ item.title }}</h3>\n          <p class=\"b-2-cl li-desc\">{{ item.desc }}</p>\n        </li>\n      </ul>\n      <div class=\"application-region\">\n        <p class=\"b-2-cl app-desc\" v-if=\"applyPass\">{{ congratulate }}</p>\n        <p class=\"b-2-cl app-desc\" v-if=\"btnShow\">\n          <c-checkBox :value=\"textFlag\" @click=\"checkoutClick\" />\n          <span class=\"protocol\"\n            >{{ $t(\"application.agree\") }}\n            <a href=\"/applicationPolicy\" target=\"_blank\" class=\" b-4-cl\">{{\n              $t(\"application.agreePolicy\")\n            }}</a>\n          </span>\n        </p>\n        <div class=\"btn-region b-4-cl\">\n          <c-button\n            v-if=\"relieving\"\n            height=\"40px\"\n            width=\"140px\"\n            @click=\"relieving\"\n            className=\"comBtn\"\n            :disabled=\"true\"\n            paddingW=\"0px\"\n            >{{ $t(\"application.relieveing\") }}\n          </c-button>\n          <c-button\n            v-if=\"btnShow\"\n            height=\"40px\"\n            width=\"140px\"\n            @click=\"apply\"\n            className=\"comBtn\"\n            :disabled=\"submitDisabled\"\n            paddingW=\"0px\"\n            >{{ applyBtn }}\n          </c-button>\n          <c-button\n            v-if=\"applyPass\"\n            :big=\"true\"\n            @click=\"relieveClick\"\n            :type=\"\n              (applyStatus === 3 && applyRuleStatus === 3) ||\n              (applyStatus === 2 && applyRuleStatus === 3)\n                ? undefined\n                : 'text'\n            \"\n            className=\"relieve-btn comBtn\"\n            paddingW=\"31px\"\n            height=\"40px\"\n            >{{ this.$t(\"application.relieve\") }}\n          </c-button>\n        </div>\n      </div>\n      <!-- 提示框 -->\n      <c-dialog\n        :showFlag=\"alertFlag\"\n        buttonText=\"btnConfirm\"\n        :titleText=\"$t('application.relieve')\"\n        @close=\"alertClose\"\n        :confirmLoading=\"confirmLoading\"\n        @confirm=\"confirm\"\n      >\n        <p class=\"b-2-cl dialog-text\">{{ $t(\"application.dialogDesc\") }}</p>\n      </c-dialog>\n    </div>\n  </div>\n</template>\n\n<script>\nimport mixin from 'BlockChain-ui-privatization/PC/common-mixin/CompanyApplication/CompanyApplication/CompanyApplication';\nimport 'BlockChain-ui-privatization/PC/common-mixin/CompanyApplication/CompanyApplication/CompanyApplication.styl';\n\nexport default {\n  mixins: [mixin],\n  mounted() {\n    this.init();\n  },\n};\n</script>\n"]}]}