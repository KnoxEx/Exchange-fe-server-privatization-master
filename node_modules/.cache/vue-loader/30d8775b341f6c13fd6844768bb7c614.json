tradeForm/tradeInput/tradeInput.styl';\n\nexport default {\n  mixins: [mixin],\n  name: 'GridTradeInput',\n  mounted() {\n    this.init();\n  },\n  props: {\n    placeText: {\n      default: null,\n      type: String,\n    },\n  },\n  watch: {\n    'content.isError': {\n      handler(v) {\n        if (v) {\n          setTimeout(() => {\n            this.$bus.$emit('errorMsg');\n          }, 2000);\n        }\n      },\n      deep: true,\n    },\n  },\n};\n\n</script>\n\n<style lang=\"stylus\">\n  .trade-grid .trade-input.trade-grid {\n    height: 50px !important;\n    position: relative;\n    margin-top 10px;\n  }\n  .trade-grid .trade-input.trade-grid .input-item {\n    height 35px !important;\n    padding-top 8px !important;\n    box-sizing: border-box;\n  }\n  .trade-grid .trade-input.trade-grid .input-item .units {\n    top 12px !important;\n  }\n  .trade-grid .trade-input.trade-grid .input-item .grid_inputs {\n    width 76%;\n    height: 18px;\n    line-height: 18px;\n    font-size: 14px;\n  }\n  .grid_name {\n    font-size 14px;\n  }\n  .trade-grid.trade-input-china-container .error-msg {\n    line-height: 20px;\n    height: auto;\n    min-height: 30px;\n    display: flex;\n    align-items: center;\n  }\n  .en_US .trade-grid .trade-input.trade-grid {\n    height 45px !important;\n  }\n</style>\n"]}]}