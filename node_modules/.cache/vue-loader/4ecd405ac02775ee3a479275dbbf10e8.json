cl\" v-if=\"theadList && theadList.length\">\n      <span\n        v-for=\"(item, index) in theadList\"\n        :key=\"'thead' + index\"\n        :style=\"{ width: cellWidth[index] + 'px' }\">\n        {{ item }}\n      </span>\n    </div>\n    <div\n      class=\"tbody\" :style=\"tbodyStyle\">\n      <vue-scroll :ops=\"ops\" ref=\"vs\" @handle-resize=\"handleResize\">\n        <div class=\"tbodyBar\" :style=\"tbodyStyle\">\n          <template v-if=\"differNUmber > 0 && type === 'asks'\">\n            <div\n              class=\"symbol-item a-5-bg-h b-2-cl\"\n              v-for=\"(item, index) in differNUmber\"\n              :key=\"index + 'diff'\">\n              <span\n                class=\"td-symbol\"\n                :class=\"priceClass\"\n                :style=\"{ width: cellWidth[0] + 'px' }\">\n                --\n              </span>\n              <span class=\"td-price\" :style=\"{ width: cellWidth[1] + 'px' }\">\n                --\n              </span>\n              <span class=\"td-rose\" :style=\"{ width: cellWidth[2] + 'px' }\">\n                --\n              </span>\n            </div>\n          </template>\n          <template v-for=\"(item, index) in dataList\">\n            <div\n              v-if=\"isShow(index)\"\n              :class=\"{ 'change-item': item.diff }\"\n              class=\"symbol-item a-5-bg-h b-2-cl\"\n              :key=\"index + 'item'\"\n            >\n              <!-- 价格 -->\n              <div class=\"bg a-21-bg\"></div>\n              <span\n                class=\"td-symbol\"\n                @click=\"handelPrice(item.price)\"\n                :class=\"priceClass\"\n                :style=\"{ width: cellWidth[0] + 'px' }\"\n              >\n                {{thousandsComma( item.price )}}\n              </span>\n              <!-- 数量 -->\n              <span class=\"td-price\" :style=\"{ width: cellWidth[1] + 'px' }\">\n                {{ thousandsComma(item.vol) }}\n                <div\n                  v-if=\"flashSwitchs\"\n                  class=\"change\"\n                  :class=\"trBgClass(item.diff)\"\n                ></div>\n              </span>\n              <!-- 累计 -->\n              <span class=\"td-rose\" :style=\"{ width: cellWidth[2] + 'px' }\">\n                {{ thousandsComma(item.total) }}\n                <b\n                  class=\"tr-bg\"\n                  :class=\"trBgClass_two\"\n                  :style=\"setWidth(item.total)\"\n                ></b>\n              </span>\n            </div>\n          </template>\n          <template v-if=\"differNUmber > 0 && type === 'buy'\">\n            <div\n              class=\"symbol-item a-5-bg-h b-2-cl\"\n              v-for=\"(item, index) in differNUmber\"\n              :key=\"index + 'diff'\"\n            >\n              <span\n                class=\"td-symbol\"\n                :class=\"priceClass\"\n                :style=\"{ width: cellWidth[0] + 'px' }\"\n              >\n                --\n              </span>\n              <span class=\"td-price\" :style=\"{ width: cellWidth[1] + 'px' }\">\n                --\n              </span>\n              <span class=\"td-rose\" :style=\"{ width: cellWidth[2] + 'px' }\">\n                --\n              </span>\n            </div>\n          </template>\n        </div>\n      </vue-scroll>\n    </div>\n  </div>\n</template>\n<script>\nimport mixin from 'blockchain-ui-privatization/PC/common-mixin/modules/coTrade/components/tableList/tableList';\nimport 'blockchain-ui-privatization/PC/common-mixin/modules/coTrade/components/tableList/tableList.styl';\n\nexport default {\n  mixins: [mixin],\n  mounted() {\n    this.init();\n  },\n};\n</script>\n<style lang=\"stylus\" scoped>\n./deep/.tr-bg {\n    height: 32px;\n  }\n</style>\n"]}]}